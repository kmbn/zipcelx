{
  "name": "zipcelx",
  "version": "1.6.0",
  "description": "JSON Transformer to create .xlsx files",
  "main": "lib/legacy.js",
  "module": "lib/module.js",
  "standalone-build": "lib/standalone.js",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watch",
    "lint": "eslint . ./",
    "ci": "npm run test && npm run lint",
    "build": "npm run ci && rimraf lib/* && rollup -c"
  },
  "jest": {
    "verbose": true,
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "transform": {
      ".js": "./node_modules/babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dixieio/zipcelx.git"
  },
  "keywords": [
    "JSON",
    "Excel",
    "xlsx"
  ],
  "author": "Lars Krieger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dixieio/zipcelx/issues"
  },
  "homepage": "https://github.com/dixieio/zipcelx#readme",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-env": "^7.25.4",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.4",
    "rollup-plugin-commonjs": "^8.4.0",
    "rollup-plugin-node-builtins": "2.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1"
  }
}
